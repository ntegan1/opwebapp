{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:css": "tailwindcss -i ./app/styles/index.server.css -o ./app/styles/index.css",
    "build:remix": "remix build",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "tailwindcss -i ./app/styles/index.server.css -o ./app/styles/index.css -w -p",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "cpu-features": "^0.0.4",
    "isbot": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ssh2": "^1.11.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
